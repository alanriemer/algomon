              </items>
            </ToolBar>
         </children>
      </Pane>
   </top>
   <bottom>
      <Pane prefHeight="300.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <HBox layoutX="32.0" layoutY="33.0" prefHeight="100.0" spacing="70.0">
               <children>
                  <Button fx:id="rattata" layoutX="62.0" layoutY="38.0" mnemonicParsing="false" onAction="#agregarRattataJugadorActual" prefHeight="65.0" prefWidth="75.0" text="Rattata" />
                  <Button fx:id="chansey" mnemonicParsing="false" onAction="#agregarChanseyJugadorActual" prefHeight="65.0" prefWidth="75.0" text="Chansey" />
