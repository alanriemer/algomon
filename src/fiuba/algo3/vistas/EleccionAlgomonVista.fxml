               </children>
            </HBox>
            <HBox layoutX="30.0" layoutY="138.0" prefHeight="100.0" spacing="70.0">
               <children>
                  <Button fx:id="bulbasaur" layoutX="62.0" layoutY="38.0" mnemonicParsing="false" onAction="#agregarBulbasaurJugadorActual" prefHeight="65.0" prefWidth="75.0" text="Bulbasaur" />
                  <Button fx:id="squirtle" layoutX="274.0" layoutY="38.0" mnemonicParsing="false" onAction="#agregarSquirtleJugadorActual" prefHeight="65.0" prefWidth="75.0" text="Squirtle" />
                  <Button fx:id="charmander" layoutX="471.0" layoutY="38.0" mnemonicParsing="false" onAction="#agregarCharmanderJugadorActual" prefHeight="65.0" prefWidth="75.0" text="Charmander" />
               </children>
            </HBox>
         </children>
      </Pane>
   </bottom>
</BorderPane>
