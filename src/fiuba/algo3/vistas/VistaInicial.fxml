<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane
    xmlns:fx="http://javafx.co/fxml/1"
    xmlns="http://javafx.com/javafx/2.2"

    >
    <children>
        <ImageView
                fx:id="imageView"
                fitHeight="400.0"
                fitWidth="600.0"
                layoutX="0"
                layoutY="0"
                pickOnBounds="true"
                preserveRatio="true"
            >
            <image>
                <Image
                    url="/resources/imagenPrincipal.jpg"
                    backgroundLoading="true"
                    />
            </image>
        </ImageView>
    </children>
</AnchorPane>